rain_fit = function(y,X,raindata){
# Fit linear model
model_lm = lm(y ~ X - 1, data = raindata)
# Fit Gamma GLM
model_gglm = glm(y ~ X - 1, family = Gamma(link = "log"), data = raindata)
return(list("Linear model" = model_lm, "Gamma GLM" = model_gglm))
}
